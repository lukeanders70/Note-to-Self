{
  "manifest_version": 2,

  "name": "Note To Self",
  "description": "Leave a reminder on any URL and be notified when you return",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "comment.html"
  },

  "icons":{
    "16":"icons-16.png", 
    "48":"icons-48.png", 
    "128":"icons-128.png" 
  },

  "permissions": [
    "activeTab",
    "storage"
  ]

}